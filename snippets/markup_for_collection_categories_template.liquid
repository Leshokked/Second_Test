{% capture listItems %}
{% for item in collections[settings.collection_categories_template].products limit: settings.collection_categories_template_limit %}
<div id="w-node-f0fe7a9b-c95d-b0ea-e0f8-53532cca1e6d-1641133e" role="listitem" class="collection-item-3 w-dyn-item">
          <div class="product-wrapper"><img src="{{ item.featured_image | img_url: 'master' }}" loading="lazy" alt="{{ item.featured_image.alt }}" class="back_image" data-commerce-type="variation-image" srcset="{{ item.featured_image | img_url: '1920x' }} 1920w,{{ item.featured_image | img_url: '1600x' }} 1600w,{{ item.featured_image | img_url: '1366x' }} 1366w,{{ item.featured_image | img_url: '1024x' }} 1024w,{{ item.featured_image | img_url: '768x' }} 768w,{{ item.featured_image | img_url: '640x' }} 640w"><img loading="lazy" data-w-id="f0fe7a9b-c95d-b0ea-e0f8-53532cca1e70" src="{{ item.featured_image | img_url: 'master' }}" alt="{{ item.featured_image.alt }}" class="image-2" data-commerce-type="variation-image" srcset="{{ item.featured_image | img_url: '1920x' }} 1920w,{{ item.featured_image | img_url: '1600x' }} 1600w,{{ item.featured_image | img_url: '1366x' }} 1366w,{{ item.featured_image | img_url: '1024x' }} 1024w,{{ item.featured_image | img_url: '768x' }} 768w,{{ item.featured_image | img_url: '640x' }} 640w"></div>
          <div class="info-wraper">
            <div>{{ item.title }}</div>
            <div data-commerce-type="variation-price">{{ item.price | money }}</div>
          </div>
        </div>
{%- endfor -%}
{% endcapture %}
{% assign items_length = listItems | strip | size %}
<div class="w-dyn-list" udy-collection="product">
      {%- unless items_length == 0 -%}<div role="list" class="collection-list-5 w-dyn-items">
        {{ listItems }}
      </div>{%- endunless -%}
      {%- if items_length == 0 -%}<div class="w-dyn-empty">
        <div>No items found.</div>
      </div>{%- endif -%}
    </div>